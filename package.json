{
    "name": "eslint-plugin-typegraphql",
    "version": "0.1.0",
    "description": "Linting rules for Typegraphql",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "author": "Michael Poulgrain",
    "main": "dist/lib/index.js",
    "scripts": {
        "clean": "rmdir /s /q dist",
        "lint": "eslint .",
        "test:mocha": "mocha src/tests/rules/*.ts --inspect -r ts-node/register",
        "build": "tsc --project tsconfig.json",
        "test": "jest input-type-name --watch --no-cache --runInBand"
    },
    "dependencies": {
        "requireindex": "^1.2.0"
    },
    "devDependencies": {
        "@tsconfig/node16-strictest-esm": "^1.0.0",
        "@types/eslint": "^8.4.1",
        "@types/requireindex": "^1.2.0",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "@typescript-eslint/utils": "^5.12.0",
        "eslint": "^8.0.1",
        "eslint-plugin-eslint-plugin": "^4.0.1",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^27.5.1",
        "mocha": "^9.1.3",
        "node-notifier": "^10.0.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "peerDependencies": {
        "eslint": ">=6"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "license": "ISC"
}
